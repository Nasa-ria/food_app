    <div class="form_abt">
        <form action="" method="post" class="search">
            <input
                class="input_abt"
                type="text"
                id="search"
                name="search"
                placeholder="search ......."
            />

            <button class="btn_abt" type="Submit">
                <i
                    class="bi bi-search"
                    style="font-size: 16px color chocolate"
                    ;
                ></i>
            </button>
        </form>
    </div>
    <p class="ppp_abt">COMMENTS ON DISPLAY</p>
    <div class="list_abt">
        <div class="abt_d">
                <% if (typeof comments != 'undefined'){  %> 
            <%	comments.forEach((comment) =>{ %>
                    <div id="card">
                <p class="p_abt"> COMMENT</p>
                <p class="abt_cm">Username : <%-comment.name %></p>
                <p class="abt_cm">Email Address : <%-comment.email %></p>
                <p class="abt_cm">Comment_Section : <%-comment.comment%></p>
                <div class="dropdown">
                    <button class="dropbtn">
                      <i class="bi bi-caret-down"></i>
                    </button>
                    <div class="dropdown-content">
                        <a href="/comments/edit/<%=comment._id %>">Edit Comment</a>
                  </div>

            </div>
                    </div>
                <% }) %>
                <%}%>
        
        </div>
    </div>
</div>
